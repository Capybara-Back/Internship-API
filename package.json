  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rm -rf dist/",
    "start": "npm run clean && npm run build && node dist/app.js",
    "dev": "NODE_ENV=development tsnd -r tsconfig-paths/register src/app.ts",
    "dev:watch": "NODE_ENV=development tsnd -r tsconfig-paths/register --respawn src/app.ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest --collectCoverage",
    "test:dev": "jest --watchAll --collectCoverage",
    "lint": "npx eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
