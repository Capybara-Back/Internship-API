    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
